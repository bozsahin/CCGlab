#!/bin/bash
lispbinary=`basename $CCGLAB_LISP|head -n1`   # local variable. use of head is for cases where you defined some options to base binary
$RLWRAP $CCGLAB_LISP --load $LALR_HOME/lalrparser.lisp --load $CCGLAB_HOME/bin/ccg.lisp --eval '(reset-globals)'  --eval "(welcome (quote $lispbinary))"
